{
  "name": "inferno-formlib",
  "version": "5.3.2",
  "description": "Form library for inferno",
  "main": "dist/cjs/index.js",
  "umd:main": "dist/umd/index.js",
  "module": "lib/index.js",
  "esnext": "src",
  "sideEffects": false,
  "scripts": {
    "test": "echo \"TODO: Write tests...\"",
    "prepublish": "npm run build",
    "postversion": "git push && git push --tags",
    "build-dev": "rm -rf ./dist && NODE_ENV=development babel src --out-dir lib --watch --source-maps",
    "build": "npm run build:clean && cross-env NODE_ENV=development npm run build:es && cross-env NODE_ENV=development npm run build:dist && cross-env NODE_ENV=production npm run build:dist:prod && cross-env NODE_ENV=production npm run build:legacy",
    "build:clean": "rm -rf dist/ && rm -rf lib/",
    "build:es": "babel src --out-dir lib --source-maps",
    "build:legacy": "babel src --out-dir dist",
    "build:dist": "rollup -c",
    "build:dist:prod": "rollup -c",
    "build-test": "rm -rf ./test/dist && NODE_ENV=development node_modules/.bin/webpack --mode development --config ./test/browser/webpack.config",
    "build-test-dev": "rm -rf ./test/dist && NODE_ENV=development node_modules/.bin/webpack --mode development --watch --config ./test/browser/webpack.config.js"
  },
  "keywords": [
    "inferno",
    "isomorphic-schema",
    "formlib"
  ],
  "author": "Sebastian Ware <sebastian@urbantalk.se> (https://github.com/jhsware)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jhsware/inferno-formlib.git"
  },
  "peerDependencies": {
    "component-registry": "^0.2.8",
    "isomorphic-schema": "^0.7.3"
  },
  "devDependencies": {
    "axios": "^0.18.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-inferno-app": "^7.1.0",
    "babel-preset-stage-2": "^6.24.1",
    "bootstrap": "^4.1.1",
    "component-registry": "^0.2.8 || ^0.3.0",
    "cross-env": "^5.2.0",
    "express": "^4.16.2",
    "formidable": "^1.2.0",
    "inferno-clone-vnode": "^5.3.0",
    "inferno-compat": "^5.3.0",
    "inferno-create-class": "^5.3.0",
    "inferno-create-element": "^5.3.0",
    "inferno-devtools": "^5.3.0",
    "inferno-router": "^5.3.0",
    "isomorphic-schema": "^0.8.3",
    "moment": "^2.20.1",
    "rollup": "^0.62.0",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-babel-minify": "^5.0.0",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-globals": "^1.2.1",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "inferno": "^5.3.0",
    "inferno-animation": "^5.1.4",
    "inferno-bootstrap": "^5.1.4",
    "inferno-popper": "^5.1.0",
    "lodash.tonumber": "^4.0.3",
    "safe-utils": "^0.2.1"
  }
}
